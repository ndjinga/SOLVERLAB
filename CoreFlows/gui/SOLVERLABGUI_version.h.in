#if !defined(__SOLVERLABGUI_VERSION_H__)
#define __SOLVERLABGUI_VERSION_H__

/*!
  Specify version of SALOME SOLVERLABGUI module, as follows

  SOLVERLABGUI_VERSION_MAJOR       : (integer) number identifying major version
  SOLVERLABGUI_VERSION_MINOR       : (integer) number identifying minor version
  SOLVERLABGUI_VERSION_MAINTENANCE : (integer) number identifying maintenance version
  SOLVERLABGUI_VERSION_STR         : (string)  complete version number "major.minor.maintenance"
  SOLVERLABGUI_VERSION             : (hex)     complete version number (major << 16) + (minor << 8) + maintenance
  SOLVERLABGUI_DEVELOPMENT         : (integer) indicates development version when set to 1
*/

#define SOLVERLABGUI_VERSION_MAJOR       @SALOMESOLVERLABGUI_MAJOR_VERSION@
#define SOLVERLABGUI_VERSION_MINOR       @SALOMESOLVERLABGUI_MINOR_VERSION@
#define SOLVERLABGUI_VERSION_MAINTENANCE @SALOMESOLVERLABGUI_PATCH_VERSION@
#define SOLVERLABGUI_VERSION_STR         "@SALOMESOLVERLABGUI_VERSION@"
#define SOLVERLABGUI_VERSION             @SALOMESOLVERLABGUI_XVERSION@
#define SOLVERLABGUI_DEVELOPMENT         @SALOMESOLVERLABGUI_VERSION_DEV@

#endif // __SOLVERLABGUI_VERSION_H__
